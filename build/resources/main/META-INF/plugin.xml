<idea-plugin>
  <version>0.1</version>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="191.6183" until-build="191.*"/>
  <id>xml.to.compose.xml2compose</id>
  <name>SDK: XML to Compose</name>
  <vendor email="gvadimmonstors3@gmail.com">KFU ITIS</vendor>
  <description>
      This plugin for generate jetpack compose view from xml files
    </description>
  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="Sample Window" secondary="true" icon="AllIcons.General.Modified" anchor="right" factoryClass="windows.FilesScreenFactory"/>
  </extensions>
  <actions>
    <group id="MyPlugin.TopMenu" text="_XML to _Compose" description="Need choose files for generate jetpack compose views">
      <add-to-group group-id="ToolsMenu" anchor="last"/>
      <action id="actions.MyAction" class="actions.MyAction" text="_MyAction" description="actions.MyAction"/>
    </group>
  </actions>
</idea-plugin>
